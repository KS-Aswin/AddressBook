<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".air" mimeType="application/vnd.adobe.air-application-installer-package+zip" />
        </staticContent>
        <rewrite>
            <rules>
                <rule name="Redirect to index" stopProcessing="true">
                    <match url="^index\.cfm$" />
                    <conditions>
                        <add input="{REQUEST_METHOD}" pattern="^POST$" negate="true" />
                    </conditions>
                    <action type="Redirect" url="index" appendQueryString="false" />
                </rule>
                <rule name="Rewrite to index" stopProcessing="true">
                    <match url="^index$" />
                    <conditions>
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="index.cfm" />
                </rule>
                <rule name="Redirect to list" stopProcessing="true">
                    <match url="^views/listPage\.cfm$" />
                    <conditions>
                        <add input="{REQUEST_METHOD}" pattern="^POST$" negate="true" />
                    </conditions>
                    <action type="Redirect" url="views/listPage" appendQueryString="false" />
                </rule>
                <rule name="Rewrite to list" stopProcessing="true">
                    <match url="^views/listPage$" />
                    <conditions>
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="views/listPage.cfm" />
                </rule>
                <rule name="Redirect to pdfFile" stopProcessing="true">
                    <match url="^views/pdfFile\.cfm$" />
                    <conditions>
                        <add input="{REQUEST_METHOD}" pattern="^POST$" negate="true" />
                    </conditions>
                    <action type="Redirect" url="views/pdfFile" appendQueryString="false" />
                </rule>
                <rule name="Rewrite to pdfFile" stopProcessing="true">
                    <match url="^views/pdfFile$" />
                    <conditions>
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="views/pdfFile.cfm" />
                </rule>
                <rule name="Redirect to xlFile" stopProcessing="true">
                    <match url="^views/xlFile\.cfm$" />
                    <conditions>
                        <add input="{REQUEST_METHOD}" pattern="^POST$" negate="true" />
                    </conditions>
                    <action type="Redirect" url="views/xlFile" appendQueryString="false" />
                </rule>
                <rule name="Rewrite to xlFile" stopProcessing="true">
                    <match url="^views/xlFile$" />
                    <conditions>
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="views/xlFile.cfm" />
                </rule>
                <rule name="RedirectUserFriendlyURL1" stopProcessing="true">
                    <match url="^views/signUpPage\.cfm$" />
                    <conditions>
                        <add input="{REQUEST_METHOD}" pattern="^POST$" negate="true" />
                    </conditions>
                    <action type="Redirect" url="views/signUpPage" appendQueryString="false" />
                </rule>
                <rule name="RewriteUserFriendlyURL1" stopProcessing="true">
                    <match url="^views/signUpPage$" />
                    <conditions>
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="views/signUpPage.cfm" />
                </rule>
                <rule name="Redirect to Custom Error Page" stopProcessing="true">
                    <match url=".*" />
                    <conditions>
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="views/errorPage.cfm" />
                </rule>
            </rules>
            <outboundRules>
                <rule name="OutboundRewriteIndex" preCondition="ResponseIsHtml1">
                    <match filterByTags="A, Form, Img" pattern="^(.*)index\.cfm$" />
                    <action type="Rewrite" value="{R:1}/index" />
                </rule>
                <rule name="OutboundRewriteListPage" preCondition="ResponseIsHtml1">
                    <match filterByTags="A, Form, Img" pattern="^(.*)views/listPage\.cfm$" />
                    <action type="Rewrite" value="{R:1}/views/listPage" />
                </rule>
                <rule name="OutboundRewritePdfFile" preCondition="ResponseIsHtml1">
                    <match filterByTags="A, Form, Img" pattern="^(.*)views/pdfFile\.cfm$" />
                    <action type="Rewrite" value="{R:1}/views/pdfFile" />
                </rule>
                <rule name="OutboundRewriteXlFile" preCondition="ResponseIsHtml1">
                    <match filterByTags="A, Form, Img" pattern="^(.*)views/xlFile\.cfm$" />
                    <action type="Rewrite" value="{R:1}/views/xlFile" />
                </rule>
                <rule name="OutboundRewriteUserFriendlyURL1" preCondition="ResponseIsHtml1">
                    <match filterByTags="A, Form, Img" pattern="^(.*)views/signUpPage\.cfm$" />
                    <action type="Rewrite" value="/views/signUpPage" />
                </rule>
                <preConditions>
                    <preCondition name="ResponseIsHtml1">
                        <add input="{RESPONSE_CONTENT_TYPE}" pattern="^text/html" />
                    </preCondition>
                </preConditions>
            </outboundRules>
        </rewrite>
        <httpErrors errorMode="Custom">
            <remove statusCode="404" subStatusCode="-1" />
            <error statusCode="404" path="/views/errorPage.cfm" responseMode="ExecuteURL" />
        </httpErrors>
    </system.webServer>
</configuration>
